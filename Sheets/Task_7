Q1)read and print array
#include <stdio.h>

int main()
{
    int n;
    printf("Enter the range\n");
    scanf("%d",&n);
    int a[n];
    printf("Enter number\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        printf("%d\t",a[i]);
    }
    return 0;
}
Q2)print all negative element
#include <stdio.h>

int main()
{
    int n;
    printf("Enter the range\n");
    scanf("%d",&n);
    int a[n];
    printf("Enter negative number\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        if(a[i]<0)
        printf("%d\t",a[i]);
    }
    return 0;
}
Q3)sum of the numbers
#include <stdio.h>

int main()
{
    int n,sum;
    printf("Enter the range\n");
    scanf("%d",&n);
    int a[n];
    printf("Enter number\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        sum=sum+a[i];
    }
    printf("sum of the number is %d",sum);
    return 0;
}
Q4)Maximum and Minimum value
#include <stdio.h>

int main()
{
    int n,max,min;
    printf("Enter the range\n");
    scanf("%d",&n);
    int a[n];
    printf("Enter number\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        min=a[i];
        if(a[i]>max)
        max=a[i];
        if(a[i]<min)
        min=a[i];
    }
    printf("Maximum and Minimum values are %d and %d",max,min);
    return 0;
}
