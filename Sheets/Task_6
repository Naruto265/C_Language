1. Palindrome
#include <stdio.h>
#include<math.h>

int main()
{
    int i,a=0,b=0,c=0;
    printf("Enetr the number ");
    scanf("%d",&i);
    c=i;
    while(i>0)
    {
        a=i%10;
        b=a+(b*10);
        i=i/10;
    }
    if(c==b)
    {
    printf("It is a palindrome number");
    }
    else
    {printf("It is not a palindrome number");
}
    return 0;
}

2. Prime number or not
#include <stdio.h>

int main()
{
    int n,count;
    printf("Enter a number ");
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        if(n%i==0)
        count+=1;
    }
    if(count==2)
    printf("It is a prime number");
    else
    printf("It is not a prime number");

    return 0;
}

3.Armstrong number
#include <stdio.h>
#include<math.h>

int main()
{
    int n,count=0,c=0,a=0,b=0;
    printf("Enter a number ");
    scanf("%d",&n);
    c=n;
    count=log10(n)+1;
    while(n>0)
    {
    a=n%10;
    b=b+(pow(a,count));
    n=n/10;
    }
    if(c==b)
    printf("It is an armstrong number");
    else
    printf("It is not a armstrong number");
    return 0;
}
